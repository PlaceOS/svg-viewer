{
    "version": "0.5.4-dev",
    "license": "MIT",
    "name": "@placeos/svg-viewer",
    "author": "Alex Sorafumo <alex@yuion.net>",
    "type": "module",
    "main": "dist/index.cjs.js",
    "module": "dist/index.es.js",
    "types": "dist/index.d.ts",
    "keywords": [
        "placeos",
        "browser",
        "api"
    ],
    "files": [
        "dist",
        "src"
    ],
    "engines": {
        "node": ">=20"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/PlaceOS/svg-viewer.git"
    },
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "docs": "typedoc --excludePrivate --out docs src",
        "lint": "eslint .",
        "test": "vitest",
        "test:cov": "vitest run --coverage",
        "format": "prettier --write .",
        "semantic-release": "semantic-release",
        "prepare": "npm run build",
        "demo": "npm run build && cp ./demo/australia.svg ./dist/ && parcel demo/index.html"
    },
    "devDependencies": {
        "@vitest/coverage-v8": "^3.0.7",
        "date-fns": "^2.15.0",
        "jsdom": "^26.0.0",
        "prettier-plugin-organize-imports": "^4.1.0",
        "rxjs": "^7.8.2",
        "semantic-release": "^24.2.3",
        "ts-md5": "^2.0.1",
        "typedoc": "^0.27.9",
        "typescript": "^5.8.0",
        "vite": "^6.2.0",
        "vite-plugin-dts": "^4.5.3",
        "vitest": "^3.0.7"
    },
    "peerDependencies": {
        "rxjs": "^7.8.2",
        "ts-md5": "^2.0.1"
    }
}
